CREATE TABLE lines (
	lineId integer NOT NULL,
	lineName varchar(50) NOT NULL,
	lineStartStop varchar(50) NOT NULL,
	/* co ked je tych startTimeov viac  */
	--ja mam kazdu linku tolkokrat v tabulke kolko je startimov, ze kazda cesta je brana ako iny zaznam v tabulke
	lineStartTime time NOT NULL,
)

CREATE TABLE linesStops (
	lineId integer NOT NULL,
	stopName varchar(50) NOT NULL,
	stopOrder integer NOT NULL,
	stopTimeDiff time NOT NULL,
	stopTime time NOT NULL,
	numberOfPasengers integer,
	capacity integer NOT NULL
	
)

